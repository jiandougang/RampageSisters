<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RPBottomBar">
        <attr name="srcArray" format="reference" />
        <attr name="textArray" format="reference" />
    </declare-styleable>

    <declare-styleable name="pull">
        <attr name="header" format="reference|integer" />
        <attr name="maxHeaderHeight" format="reference|dimension" />
        <attr name="minHeaderHeight" format="reference|dimension" />
    </declare-styleable>

    <declare-styleable name="ParallaxListView">
        <attr name="enableZoom" format="boolean"/> <!--default=false-->
        <attr name="zoomFactor" format="float"/> <!--default=1.0-->
        <attr name="scrollMultiplier" format="float"/> <!--default=0.5-->
    </declare-styleable>
</resources>
